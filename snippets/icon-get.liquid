{%-
  liquid
  case icon
    when 'truck'
      render 'icon-truck'
    when 'card'
      render 'icon-card'
    when 'cash'
      render 'icon-cash'
    when 'shield-check'
      render 'icon-shield-check'
    when 'globe'
      render 'icon-globe'
    when 'pin'
      render 'icon-pin'
    when 'lock'
      render 'icon-lock'
  endcase
-%}
